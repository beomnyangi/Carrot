<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:background="@drawable/home_item_style"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:orientation="vertical">
	
	<TextView
		android:id="@+id/tv_blank"
		android:layout_width="100dp"
		android:layout_height="150dp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintVertical_bias="0.0"
		app:layout_constraintHorizontal_bias="0.498"/>
	<ImageView
		android:layout_width="110dp"
		android:layout_height="110dp"
		android:scaleType="fitXY"
		tools:srcCompat="@tools:sample/avatars"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		android:id="@+id/iv_image"
		app:layout_constraintHorizontal_bias="0.05"/>
	<TextView
		android:text="제목"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		app:layout_constraintTop_toTopOf="@+id/iv_image"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		android:id="@+id/tv_title"
		app:layout_constraintStart_toEndOf="@+id/iv_image"
		app:layout_constraintHorizontal_bias="0.1"
		app:layout_constraintVertical_bias="0.05"
		android:textSize="20sp"
		android:textStyle="bold"/>
	<TextView
		android:text="동네이름"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		app:layout_constraintStart_toStartOf="@+id/tv_title"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		android:id="@+id/tv_town"
		app:layout_constraintTop_toBottomOf="@+id/tv_title"
		app:layout_constraintHorizontal_bias="0.0"
		app:layout_constraintVertical_bias="0.01"/>
	<TextView
		android:text="10,000"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		app:layout_constraintStart_toStartOf="@+id/tv_title"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		android:id="@+id/tv_price"
		app:layout_constraintTop_toBottomOf="@+id/tv_town"
		app:layout_constraintHorizontal_bias="0.0"
		app:layout_constraintVertical_bias="0.01"
		android:textStyle="bold"/>
	<ImageView
		android:layout_width="20dp"
		android:layout_height="20dp"
		android:scaleType="fitXY"
		app:srcCompat="@drawable/outline_question_answer_24"
		app:layout_constraintTop_toTopOf="@+id/tv_chatting_count"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintBottom_toBottomOf="@+id/tv_chatting_count"
		android:id="@+id/iv_chatting"
		app:layout_constraintEnd_toStartOf="@+id/tv_chatting_count"
		app:layout_constraintHorizontal_bias="1.0"/>
	<ImageView
		android:layout_width="20dp"
		android:layout_height="20dp"
		android:scaleType="fitXY"
		app:srcCompat="@drawable/baseline_favorite_border_24"
		app:layout_constraintTop_toTopOf="@+id/tv_heart_count"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintBottom_toBottomOf="@+id/tv_heart_count"
		android:id="@+id/iv_heart"
		app:layout_constraintHorizontal_bias="1.0"
		app:layout_constraintEnd_toStartOf="@+id/tv_heart_count"/>
	<TextView
		android:text="1"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/tv_heart_count"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.9"
		app:layout_constraintHorizontal_bias="0.95"
		app:layout_constraintStart_toStartOf="parent"/>
	<TextView
		android:text="1"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/tv_chatting_count"
		app:layout_constraintEnd_toStartOf="@+id/iv_heart"
		app:layout_constraintBottom_toBottomOf="@+id/iv_heart"
		app:layout_constraintTop_toTopOf="@+id/iv_heart"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintHorizontal_bias="1.0"/>
	<TextView
		android:text="원"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/tv_won"
		app:layout_constraintStart_toEndOf="@+id/tv_price"
		app:layout_constraintBottom_toBottomOf="@+id/tv_price"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintTop_toTopOf="@+id/tv_price"
		app:layout_constraintHorizontal_bias="0.0"
		android:textStyle="bold"/>
	<ImageView
		android:layout_width="30dp"
		android:layout_height="30dp"
		app:srcCompat="@drawable/heart_red_icon"
		app:layout_constraintStart_toStartOf="@+id/lo_heart_icon"
		app:layout_constraintBottom_toBottomOf="@+id/lo_heart_icon"
		app:layout_constraintEnd_toEndOf="@+id/lo_heart_icon"
		android:id="@+id/iv_heart_icon"
		app:layout_constraintTop_toTopOf="@+id/lo_heart_icon"
	/>
	<androidx.constraintlayout.widget.ConstraintLayout
		android:layout_width="40dp"
		android:layout_height="40dp"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintTop_toTopOf="@+id/iv_image"
		app:layout_constraintStart_toStartOf="parent"
		android:id="@+id/lo_heart_icon"
		app:layout_constraintVertical_bias="0.0"
		app:layout_constraintHorizontal_bias="0.95">
	
	</androidx.constraintlayout.widget.ConstraintLayout>
	<TextView
		android:text="TextView"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/tv_SalesStatus"
		android:textStyle="bold"
		app:layout_constraintStart_toEndOf="@+id/tv_won"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintTop_toTopOf="@+id/tv_won"
		app:layout_constraintBottom_toBottomOf="@+id/tv_won"
		app:layout_constraintHorizontal_bias="0.1"/>
</androidx.constraintlayout.widget.ConstraintLayout>